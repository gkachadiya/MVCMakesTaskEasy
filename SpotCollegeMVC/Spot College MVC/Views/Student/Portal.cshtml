                                                }
                                            </li>
                                            <li>
                                                <label>Profile Created On :</label>
                                                <span>@std.CreatedDate.Value.ToShortDateString()</span>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="morebtn_">
                                        @if (CookieHelper.UserType == ((int)LoginTypes.Student).ToString())
                                        {
