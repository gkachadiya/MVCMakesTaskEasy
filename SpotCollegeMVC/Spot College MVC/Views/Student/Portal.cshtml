                                            <li>
                                                <label>Desired Start Date :</label>
                                                @if (std.StartDate != 0 && std.LookingFor != null)
                                                {
                                                    <span>@SpotCollege.Common.EnumHelper.GetDescriptionFromEnumValue((SpotCollege.Common.expectedStart)@std.StartDate).ToString()</span>
                                                }
                                                else
